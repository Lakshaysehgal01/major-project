<%if(msg&&msg.length){%>
<div
  class="alert alert-success alert-dismissible fade show mt-2 col-8 offset-2"
  role="alert"
>
  <%=msg%>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%> <%if(delMsg&&delMsg.length){%>
